# Sentiment Trading Agent - Environment Variables
# Copy this to .env and fill in your keys

# Multiple Gemini keys (comma-separated) for automatic rotation on quota limits
GEMINI_API_KEYS=key1,key2,key3
# Or single key (backward compatible)
# GEMINI_API_KEY=your_gemini_api_key_here

FINNHUB_API_KEY=your_finnhub_api_key_here
NEWSAPI_KEY=your_newsapi_key_here

# ── Admin Interface ─────────────────────────────────────────────────────────
# Credentials for the /admin panel (dataset upload & model training)
ADMIN_USERNAME=your_admin_username
ADMIN_PASSWORD=your_admin_password
ADMIN_SECRET=your_admin_secret_key
